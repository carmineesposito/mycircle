            <action method="addJs"><script>mycircle/jquery-ui-1.11.2/jquery-ui.min.js</script></action>
            <action method="addJs"><script>mycircle/no-conflict.js</script></action>
            <action method="addJs"><script>mycircle/jquery-ui.multidatespicker.js</script></action>
